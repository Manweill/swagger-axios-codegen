{
  "name": "swagger-axios-codegen",
  "version": "2.0.0",
  "main": "./dist/index",
  "typings": "./dist/",
  "author": "Manweill <manweill@outlook.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Manweill/swagger-axios-codegen.git",
    "type": "git"
  },
  "keywords": [
    "swagger",
    "axios",
    "typescript",
    "generator",
    "codegen"
  ],
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "format": "prettier --write --config .prettierrc ./src/*.ts",
    "precommit": "npm run lint && npm run format",
    "start": "node ./dist/index",
    "dev": "tsc --watch",
    "prepublish": "npm run build",
    "lint": "eslint src --fix --quiet"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "camelcase": "^5.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "multimatch": "^4.0.0",
    "pascalcase": "^0.1.1",
    "prettier": "^2.2.1",
    "structured-log": "^0.2.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "@": "./dist"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^12.7.2",
    "@types/prettier": "^1.12.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "typescript": "4.2.2"
  }
}
